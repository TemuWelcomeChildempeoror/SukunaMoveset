if v:IsA('ParticleEmitter') then 
v.Color =
ColorSequence.new{ColorSequenceKeypoint.new(0.00, 
Color3.fromRGB(255, 0, 0)), 
ColorSequenceKeypoint.new(1.00, 
Color3.fromRGB(255, 0, 0))}
end
end
